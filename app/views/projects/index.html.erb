<h1>Projects</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Date</th>
            <th>Client</th>
            <th>Project</th>
            <th>Project Code</th>
            <th>Hours</th>
            <th>Billable?</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Billable Rate</th>
        </tr>
    </thead>
    <tbody>
        <% @projects.each do |project| %>
            <tr>
                <td>
                    <%= project.date %>
                </td>
                <td>
                    <%= project.client %>
                </td>
                <td>
                    <%= project.project %>
                </td>
                <td>
                    <%= project.project_code %>
                </td>
                <td>
                    <%= project.hours %>
                </td>
                <td>
                    <%= project.billable %>
                </td>
                <td>
                    <%= project.first_name %>
                </td>
                <td>
                    <%= project.last_name %>
                </td>
                <td>
                    <%= project.billable_rate %>
                </td>
            </tr>
            <% end %>
    </tbody>
</table>